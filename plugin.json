{
    "version"           :   "1.0.1",
    "name"              :   "O46theme",
    "pluginIcon"        :   "icon_plugin_xs.png",
    "authorIcon"        :   "icon_author_xs.png",
    "email"             :   "marcel@outlet46.de",
    "phone"             :   "+49 53213896790",
    "description"       :   "A O46 Theme for Ceres 3.",
    "namespace"         :   "O46theme",
    "author"            :   "Smasher",
    "type"              :   "theme",
    "require"           :   {
                                "Ceres":">= 3.0.0"
                            },
    "serviceProvider"   :   "O46theme\\Providers\\O46themeServiceProvider",
